{

  "description": "TEST 2",
  "manifest_version": 2,
  "name": "poopicorn",
  "version": "1.0",
  "icons": {
    "48": "icons/Poopicorn_logo_48px.png"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icons/Poopicorn_logo_48px.png",
    "default_title": "poopicorn",
    "default_popup": "popup/poopi.html"
  },


  "background": {
    "scripts": [ "socket.io.min.js", "background.js"]
  },

  "web_accessible_resources": [
    "images/Poop-o-meter.png",
    "images/Poopicorn_48px.png"
  ]


}
